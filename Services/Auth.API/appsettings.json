{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtOptions": {
    "Secret": "YourSuperSecretKeyHere", // Replace with a strong secret key
    "Issuer": "YourAppName", // Usually your application name
    "Audience": "YourAudience", // Specify the intended audience
    "ExpirationMinutes": 60 // Token expiration in minutes
  }
}
